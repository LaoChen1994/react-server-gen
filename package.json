  "name": "my-cli",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npx nodemon --config ./nodemon.json",
    "dev": "rm -r ./local/* || : && cd ./client && npm run dev",
    "build": "yarn build-server && yarn build-client",
    "build-server": "rm -r ./config/version* || : && rm -r ./dist ||: && mkdir dist && npx tsc && cd ./dist && mkdir views && cp -r ../app/views/ ./views/",
    "build-client": "rm -r ./config/version* || : && rm -r ./local || : && mkdir local && cd ./client && npm run build",
    "dep:install": "yarn && cd ./client && yarn",
    "start:prod": "/bin/sh ./script/start.sh"
  },
